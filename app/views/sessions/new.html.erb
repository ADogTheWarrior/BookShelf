Login

<%= link_to "Log In with Google", '/auth/google_oauth2' %>
<!-- have this code on the new user view as well -->

<%= form_tag sessions_path do %>
  Email: <%= select_tag "email", options_from_collection_for_select(User.order(:email), 'email', 'email')%><br>
  Password: <%= password_field_tag "password" %>
  <%= submit_tag "Sign In" %>
<% end %>
